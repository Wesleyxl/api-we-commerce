{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/no-unresolved": "off",
    "consistent-return": "warn",
    "camelcase": "off",
    "no-unused-vars": "warn"
  }
}
